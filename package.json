{
  "name": "web-lib-starter",
  "version": "0.1.0",
  "description": "Web Lib Starter - A boilerplate for creating a Web library.",
  "main": "lib/index.js",
  "module": "es/index.js",
  "author": "senntyou <jiangjinbelief@163.com>",
  "homepage": "https://github.com/senntyou/web-lib-starter",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/senntyou/web-lib-starter"
  },
  "keywords": [
    "web",
    "lib",
    "library",
    "starter",
    "boilerplate",
    "webpack",
    "gulp",
    "jest"
  ],
  "bugs": {
    "url": "https://github.com/senntyou/web-lib-starter/issues"
  },
  "readmeFilename": "README.md",
  "files": [
    "lib",
    "es"
  ],
  "scripts": {
    "precommit": "npm run lint-staged",
    "lint-staged": "lint-staged",
    "prettier": "prettier --write \"./**/*.{js,css,less,scss,sass,html,md,json}\"",
    "eslint": "eslint \"./**/*.js\"",
    "eslint:fix": "eslint \"./**/*.js\" --fix",
    "stylelint": "stylelint \"./**/*.{css,less,scss,sass}\"",
    "stylelint:fix": "stylelint \"./**/*.{css,less,scss,sass}\" --fix",
    "flow": "flow",
    "flow:check": "flow check",
    "build:less": "gulp less",
    "build:scss": "gulp scss",
    "build:styles": "npm run build:less",
    "build:lib": "gulp lib",
    "build:es": "gulp es",
    "build": "npm run clean && npm run build:styles && npm run build:lib && npm run build:es",
    "clean": "rimraf lib es",
    "prepublish": "npm run build",
    "test": "jest",
    "test-watch": "jest --watch --notify",
    "test-cover": "jest --coverage",
    "coverage": "npm run test-cover && opn coverage/lcov-report/index.html"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-flow": "^7.0.0",
    "autoprefixer": "^9.4.10",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "browser-sync": "^2.26.3",
    "css-loader": "^2.1.1",
    "eslint": "^5.12.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.4.0",
    "eslint-plugin-flowtype": "^3.2.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.1.3",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "file-loader": "^3.0.1",
    "flow-bin": "^0.94.0",
    "fs-extra": "^7.0.1",
    "gulp": "^4.0.0",
    "gulp-babel": "^8.0.0",
    "gulp-less": "^4.0.1",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^4.0.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.1.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "lila-bin": "^0.7.1",
    "lila-core": "^0.7.1",
    "lila-tasks": "^0.7.1",
    "lila-webpack": "^0.7.1",
    "lint-staged": "^8.1.0",
    "lodash": "^4.17.11",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "opn-cli": "^4.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.15.3",
    "rd": "^2.0.1",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "speed-measure-webpack-plugin": "^1.3.0",
    "style-loader": "^0.23.1",
    "stylelint": "^9.9.0",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-standard": "^18.2.0",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "url-loader": "^1.1.2",
    "webpack-merge": "^4.2.1",
    "webpackbar": "^3.1.5"
  },
  "lint-staged": {
    "**/*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "**/*.{css,less,scss,sass}": [
      "stylelint --fix",
      "prettier --write",
      "git add"
    ],
    "**/*.{html,md,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.3.4"
  }
}
